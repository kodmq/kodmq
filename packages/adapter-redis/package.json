{
  "name": "@kodmq/adapter-redis",
  "version": "1.0.0-beta.3",
  "description": "KodMQ adapter for Redis",
  "keywords": [
    "redis",
    "message queue",
    "background jobs"
  ],
  "author": "Andrew Kodkod <andrew@kodkod.me> (https://kodkod.me/)",
  "homepage": "https://kodmq.com",
  "repository": "kodmq/kodmq",
  "bugs": "https://github.com/kodmq/kodmq/issues",
  "funding": "https://github.com/sponsors/akodkod",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "test": "tap --timeout=5 --jobs=1 --coverage-report=lcov",
    "tsc": "tsc -p ./tsconfig.lint.json",
    "build": "rollup -c && tsc"
  },
  "devDependencies": {
    "@kodmq/core": "workspace:*",
    "@types/node": "20.6.2",
    "eslint-config-kodmq": "workspace:*",
    "jest": "^29.7.0",
    "rollup": "^3.29.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-esbuild": "6.0.1",
    "tap": "^18.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@kodmq/core": "^1.0.0-beta.3"
  },
  "dependencies": {
    "ioredis": "^5.3.2"
  }
}

{
  "name": "kodmq",
  "version": "0.5.0",
  "type": "module",
  "description": "A simple message queue",
  "main": "./dist/kodmq.js",
  "types": "./dist/kodmq.d.ts",
  "exports": {
    ".": {
      "types": "./dist/kodmq.d.ts",
      "import": "./dist/kodmq.js",
      "require": "./dist/kodmq.js"
    },
    "./launcher": {
      "types": "./dist/launcher/index.d.ts",
      "import": "./dist/launcher/index.js",
      "require": "./dist/launcher/index.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js",
      "require": "./dist/errors.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js",
      "require": "./dist/constants.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "launcher": [
        "./dist/launcher/index.d.ts"
      ],
      "errors": [
        "./dist/errors.d.ts"
      ],
      "constants": [
        "./dist/constants.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --detectOpenHandles",
    "tsc": "tsc -p ./tsconfig.lint.json",
    "build": "rollup -c && tsc"
  },
  "devDependencies": {
    "@types/node": "20.6.2",
    "eslint-config-kodmq": "workspace:*",
    "jest": "^29.7.0",
    "rollup": "^3.29.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-esbuild": "6.0.1",
    "ts-jest": "^29.1.1"
  },
  "peerDependencies": {
    "ioredis": "^5.3.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "node-graceful-shutdown": "^1.1.5"
  }
}

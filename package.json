    "chalk": "^5.3.0",
    "concurrently": "^8.2.1",
    "eslint-config-kodmq": "workspace:^",
    "git-branch": "^2.0.1",
    "git-parse": "^3.0.1",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",

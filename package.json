{
  "name": "kodmq-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "tsc": "turbo run tsc",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/workspaces": true
    }
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@release-it-plugins/workspaces": "^4.0.0",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "release-it": "^16.2.0",
    "ts-jest": "^29.1.1",
    "turbo": "^1.10.14",
    "typescript": "^5.2.2"
  },
  "version": "0.5.0"
}
